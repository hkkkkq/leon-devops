include crm2_setting.conf;
include crm2_odds_news.conf;        
include crm2_path.conf;
include crm2_redirect_to_mobile.conf;


location /brand/ {
    proxy_buffering        on;
    proxy_cache_valid      200  14d;
    proxy_cache            STATIC;

    rewrite ^(.*)$ /vc3/prod/brand$1 break;
    include crm2_frontend_s3bucket.conf;
    proxy_redirect /vc3/prod/brand/ /;
}

location /jetsostatic/ {
    proxy_buffering        on;
    proxy_cache_valid      200  1m;
    proxy_cache            STATIC;

    rewrite ^(.*)$ /vc3/prod/jetsostatic$1 break;
    include crm2_frontend_s3bucket.conf;
    proxy_redirect /vc3/prod/jetsostatic/ /;
}

location / {
    proxy_buffering        on;
    proxy_cache_valid      200  14d;
    proxy_cache            STATIC;

    proxy_intercept_errors on;
    error_page 404 @reactindexhtml;

    rewrite ^(.*)$ /vc3/prod/mobile188appsapk$1 break;
    include crm2_frontend_s3bucket.conf;
    proxy_redirect /vc3/prod/mobile188appsapk/ /;
}

location @reactindexhtml {
    proxy_buffering        on;
    proxy_cache_valid      200  14d;
    proxy_cache            STATIC;

    rewrite ^(.*)$  /vc3/prod/mobile188appsapk/mobile/index.html break;
    include crm2_frontend_s3bucket.conf;
    header_filter_by_lua '
        ngx.status = 200
    ';
}

location = /mobile/static/js/app-service.js {
    rewrite ^(.*)$  /vc3/prod/mobile188appsapk/mobile/static/js/app-service.js break;
    include crm2_frontend_s3bucket.conf;
	sub_filter_types *;         
	sub_filter "var isOurApp = false;"  "var isOurApp = true; console.log('modified my sub_filter aaa');"  ;
}




#    tell the user to what type of module this domain belongs
#    通知用戶，這是甚麼類型的域名
location = /servingpage/which193.json{
   set $module_type "mobile apk office app";
   
   return 200 "{    \"domain\":  \"$host\",
                    \"server port\":  \"$server_port\", 
                    \"type\":  \"$module_type\"    }";
}